    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^2.17.0"
  },
  "devDependencies": {
    "@types/chosen-js": "^1.8.0",
    "@types/jest": "^23.3.1",
    "@types/jquery": "^3.3.6",
    "@types/lodash": "^4.14.116",
    "@types/node": "^8.10.28",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-router-dom": "^4.3.1",
    "husky": "^1.0.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react-app-rewire-less": "^2.1.2",
    "react-app-rewired": "^1.6.2",
