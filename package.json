{
  "name": "@luotao/learning-react",
  "version": "0.1.0",
  "private": true,
  "description": "@luotao/learning-react",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "codecov": "codecov --token=2797cb08-8345-433d-8007-f75db0314dee --disable=gcov",
    "fetch:blocks": "fetch-blocks",
    "format-imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write **/*",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "ui": "umi ui"
  },
  "dependencies": {
    "@ant-design/pro-layout": "^4.5.11",
    "@antv/data-set": "^0.10.2",
    "antd": "^3.20.7",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.0.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "redux": "^4.0.4",
    "umi": "^2.8.16",
    "umi-plugin-pro-block": "^1.3.4",
    "umi-plugin-react": "^1.9.14",
    "umi-request": "^1.2.1"
  },
  "devDependencies": {
    "@ant-design/colors": "^3.1.0",
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.16",
    "@types/lodash": "^4.14.136",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.24",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.5",
    "@umijs/fabric": "^1.1.9",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "codecov": "^3.5.0",
    "cross-env": "^6.0.0",
    "cross-port-killer": "^1.1.1",
    "enzyme": "^3.9.0",
    "eslint": "^5.16.0",
    "fetch-blocks": "^1.0.3",
    "gh-pages": "^2.1.0",
    "husky": "^2.3.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.3.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^8.1.7",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^2.6.0",
    "prettier": "^1.17.1",
    "pro-download": "1.0.1",
    "slash2": "^2.0.0",
    "stylelint": "^10.1.0",
    "umi-plugin-ga": "^1.1.5",
    "umi-plugin-pro": "^1.0.2",
    "umi-types": "^0.3.15",
    "webpack-theme-color-replacer": "^1.2.17"
  },
  "optionalDependencies": {
    "puppeteer": "^1.17.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
